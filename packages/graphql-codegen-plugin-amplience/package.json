{
    "name": "graphql-codegen-plugin-amplience",
    "author": "Boris Besemer",
    "version": "0.1.0",
    "module": "dist/graphql-codegen-plugin-amplience.esm.js",
    "private": true,
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
      "dist",
      "src"
    ],
    "engines": {
      "node": ">=10"
    },
    "scripts": {
      "start": "tsdx watch",
      "build": "tsdx build",
      "test": "tsdx test --passWithNoTests",
      "lint": "tsdx lint",
      "prepare": "tsdx build",
      "size": "size-limit",
      "analyze": "size-limit --why",
      "example": "graphql-codegen --config examples/codegen.yml"
    },
    "peerDependencies": {},
    "husky": {
      "hooks": {
        "pre-commit": "tsdx lint"
      }
    },
    "prettier": {
      "printWidth": 80,
      "semi": true,
      "singleQuote": true,
      "trailingComma": "es5"
    },

    "size-limit": [
      {
        "path": "dist/graphql-codegen-plugin-amplience.cjs.production.min.js",
        "limit": "10 KB"
      },
      {
        "path": "dist/graphql-codegen-plugin-amplience.esm.js",
        "limit": "10 KB"
      }
    ],
    "devDependencies": {},
    "dependencies": {},
    "workspaces": ["packages/*"]
  }
