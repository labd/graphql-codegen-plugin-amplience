{
  "$id": "https://schema-examples.com/content-type",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "allOf": [
    { "$ref": "http://bigcontent.io/cms/schema/v1/core#/definitions/content" }
  ],
  "title": "Content Type",
  "properties": {
    "_meta": {
      "type": "object",
      "title": "Delivery Key",
      "properties": {
        "deliveryKey": {
          "type": "string",
          "title": "test title",
          "description": "test description",
          "pattern": "test pattern"
        }
      }
    },
    "b": { "title": "B", "type": "integer" },
    "h": { "title": "H", "type": "array", "items": { "type": "string" } },
    "z": { "title": "Z", "description": "This a description", "type": "string" }
  },
  "description": "Content Type",
  "propertyOrder": ["_meta", "b", "h", "z"],
  "required": ["b", "h"]
}
